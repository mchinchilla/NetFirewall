{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AppConfig": {
    "CheckInterval": 20
  },
  "BashCommands": {
    "ExtraPrimaryCommands": [
      "/usr/sbin/nft -f /etc/nftables.conf",
      "systemctl restart wg-quick@wg0.service",
      "/usr/bin/ip rule del not from all fwmark 0xca6c lookup 51820"
    ],
    "ExtraSecondaryCommands": [
      "/usr/sbin/nft -f /etc/claro-nftables.conf",
      "systemctl restart wg-quick@wg0.service",
      "/usr/bin/ip rule del not from all fwmark 0xca6c lookup 51820"
    ]
  },
  "Network": [
    {
      "InterfaceName": "ens192",
      "InterfaceGateway": "181.200.100.1",
      "IsPrimary": true,
      "MonitorIPs": [
        "8.8.8.8",
        "1.1.1.1"
      ]
    },
    {
      "InterfaceName": "ens224",
      "InterfaceGateway": "190.100.200.1",
      "IsPrimary": false,
      "MonitorIPs": [
        "8.8.4.4",
        "1.0.0.1"
      ]
    }
  ]
}
